{
    "name": "francisco/php_ddd_skeleton",
    "description": "PHP DDD skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Marcano",
            "email": "fjmn2001@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Medine\\Apps\\Todo\\Backend\\": "apps/todo/backend/src",
            "Medine\\Apps\\Todo\\Frontend\\": "apps/todo/frontend/src",
            "Medine\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Medine\\Tests\\": [
                "tests/src"
            ]
        }
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behat/mink-browserkit-driver": "^1.3",
        "friends-of-behat/symfony-extension": "^2.1",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.4",
        "fzaninotto/faker": "^1.9"
    }
}
